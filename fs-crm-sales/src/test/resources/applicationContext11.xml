<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="restServiceProxyFactory" class="com.facishare.rest.proxy.RestServiceProxyFactory"
          p:configName="fs-paas-appframework-rest" init-method="init"/>

    <bean id="productProxy" class="com.facishare.rest.proxy.RestServiceProxyFactoryBean"
          p:type="com.facishare.crm.sfa.utilities.proxy.ProductProxy">
        <property name="factory" ref="restServiceProxyFactory"></property>
    </bean>

    <context:annotation-config/>


</beans>
