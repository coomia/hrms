<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
			http://www.springframework.org/schema/context
			http://www.springframework.org/schema/context/spring-context-3.0.xsd
			http://www.springframework.org/schema/context
			http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <!-- ioc -->
    <context:annotation-config/>
    <context:component-scan base-package="com.facishare.crm.electronicsign.predefine.dao"/>

    <bean id="datastore" class="com.facishare.crm.electronicsign.mongo.spring.MongoDataStoreTestFactoryBean">
        <constructor-arg index="0" value="test-db"/>
        <constructor-arg index="1" value="com.facishare.crm.electronicsign.predefine.model"/>
    </bean>
</beans>